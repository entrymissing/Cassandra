[
  {
    "class": "PingProber",
    "prefix": "ping.outpost",
    "frequency": 300,
    "parameters": {
      "ping_targets": ["192.168.0.1", "google.com", "reddit.com",
                       "scripts.mit.edu", "mail.caltech.edu"],
      "num_pings": 3
    }
  },
  {
    "class": "ExternalIpProber",
    "prefix": "stats.outpost",
    "frequency": 3600
  },
  {
    "class": "GmailOldestInInboxProber",
    "prefix": "mail.private",
    "frequency": 300
  },
  {
    "class": "GmailLengthOfQueryProber",
    "prefix": "mail.private",
    "frequency": 300,
    "parameters": {
      "queries": ["in:inbox",
                  "in:sent newer_than:7d",
                  "in:sent newer_than:24h",
                  "in:sent newer_than:1h"],
      "suffixes": ["inbox_size",
                   "sent_last_7d",
                   "sent_last_24h",
                   "sent_last_1h"]
    }
  },
  {
    "class": "GFitProber",
    "prefix": "workout",
    "frequency": 300,
    "parameters": {
      "time_windows": [168,
                       720],
      "suffixes": ["7d",
                   "30d"]
    }
  }  
]
