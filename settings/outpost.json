[
  {
    "class": "GmailOldestInInboxProber",
    "prefix": "mail.private",
    "frequency": 120
  },
  {
    "class": "GmailLengthOfQueryProber",
    "prefix": "mail.private",
    "frequency": 120,
    "parameters": {
      "queries": ["in:inbox",
                  "in:sent newer_than:7d",
                  "in:sent newer_than:24h",
                  "in:sent newer_than:1h"],
      "suffixes": ["inbox_size",
                   "sent_last_7d",
                   "sent_last_24h",
                   "sent_last_1h"]
    }
  },
  {
    "class": "GFitProber",
    "prefix": "workout",
    "frequency": 120,
    "parameters": {
      "time_windows": [168,
                       720],
      "suffixes": ["7d",
                   "30d"]
    }
  },
  {
    "class": "GCalendarProber",
    "prefix": "calendar",
    "frequency": 14,
    "parameters": {
      "time_windows": [168,
                       720],
      "suffixes": ["7d",
                   "30d"]
    }
  }  
]
